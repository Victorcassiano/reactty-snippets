      "import { View, StyleSheet } from 'react-native';",
      "\nconst $1: React.FC = () => {",
      "\treturn (",
      "\t\t<View>",
      "\t\t</View>",
      "\t);",
      "}",
      " ",
      "const styles = StyleSheet.create({",
      "\t$2: {",
      "\t},",
      ")}",
      "\nexport default $1;"
    ],
    "description": "Create page for React-Native"
  },

  "Function Component with Interface end Export Default": {
    "prefix": "cwi",
    "body": [
      "export interface $1Props {",
      "\t$2",
      "}",
      " ",
      "const $1: React.FC<$1Props> = ($3) => {",
      "\treturn ( );",
      "}",
      " ",
      "export default $1;"
    ],
    "description": "Function Component with Interface end Export Default"
  },

  "Create StyleSheet": {
    "prefix": "sheet",
    "body": [
      "const styles = StylesSheet.create({",
      "\t$1: {",
      "\n\t}",
      "})"
    ],
    "description": "Create StyleSheet"
  },

  "Function": {
    "prefix": "fc",
    "body": [
      "function $1($2): $3 {",
      "\treturn ( );",
      "}"
    ],
    "description": "Function"
  },

  "Function Export Default": {
    "prefix": "fc-ex",
    "body": [
      "function $1($2) {",
      "\treturn ( $0 );",
      "}",
      "",
