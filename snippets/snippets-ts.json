{
  "Import React": {
    "prefix": "imr",
    "body": ["import React from 'react';"],
    "description": "Import React"
  },

  "Import React and Component": {
    "prefix": "imrn",
    "body": [
      "import React from 'react';",
      "import {  } from 'react-native';"
    ],
    "description": "Import React end React-Native"
  },

  "Import React, { useState }": {
    "prefix": "imrs",
    "body": [
      "import React, { useState } from 'react';"
    ],
    "description": "Import React, { useState }"
  },

  "Import React, { useState, useEffect }": {
    "prefix": "imrse",
    "body": [
      "import React, { useState, useEffect } from 'react';"
    ],
    "description": "Import React, { useState, useEffect }"
  },

  "Create Page React":{
    "prefix": "rcp",
    "body": [
      "import React from 'react';",
      "\nexport function $1(): void {",
      "\treturn (",
      "\t\t<div>",
      "\t\t</div>",
      "\t);",
      "}",
      " "
    ],
    "description": "Create page for React"
  },

  "Create Page React-Native":{
    "prefix": "crn",
    "body": [
      "import React from 'react';",
      "import { View } from 'react-native';",
      "\nexport function $1(): void {",
      "\treturn (",
      "\t\t<View>",
      "\t\t</View>",
      "\t);",
      "}"
    ],
    "description": "Create page for React-Native"
  },

  "Create Page React Export Default":{
    "prefix": "rcp-ex",
    "body": [
      "import React from 'react';",
      "\nconst $1: React.FC = () => {",
      "\treturn (",
      "\t\t<div>",
      "\t\t</div>",
      "\t);",
      "}",
      " ",
      "export default $1;"
    ],
    "description": "Create page for React Export Default"
  },

  "Create Page React-Native Export Default":{
    "prefix": "crn-ex",
    "body": [
      "import React from 'react';",
      "import { View } from 'react-native';",

      "\nconst $1: React.FC = () => {",
      "\treturn (",
      "\t\t<View>",
      "\t\t</View>",
      "\t);",
      "}",
      " ",
      "export default $1;"
    ],
    "description": "Create page for React-Native Export Default"
  },

  "Create Page React-Native with StyleSheet":{
    "prefix": "crn-sheet",
